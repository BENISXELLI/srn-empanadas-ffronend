  </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"modal"</span></span><span>).</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">display</span></span><span> = </span><span><span class="hljs-string">"block"</span></span><span>;

  </span><span><span class="hljs-keyword">if</span></span><span> (empanada) {
    </span><span><span class="hljs-comment">// Modo editar</span></span><span>
    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"modal-title"</span></span><span>).</span><span><span class="hljs-property">innerText</span></span><span> = </span><span><span class="hljs-string">"Editar Empanada"</span></span><span>;
