    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"empanada-id"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"empanada-nombre"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"empanada-tipo"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;
    </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"empanada-precio"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span> = </span><span><span class="hljs-string">""</span></span><span>;
  }
}

</span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">cerrarModal</span></span><span>(</span><span><span class="hljs-params"></span></span><span>) {
  </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"modal"</span></span><span>).</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">display</span></span><span> = </span><span><span class="hljs-string">"none"</span></span><span>;
}

</span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"empanada-form"</span></span><span>).</span><span><span class="hljs-title function_">addEventListener</span></span><span>(</span><span><span class="hljs-string">"submit"</span></span><span>, </span><span><span class="hljs-keyword">async</span></span><span> (e) =&gt; {
  e.</span><span><span class="hljs-title function_">preventDefault</span></span><span>();

  </span><span><span class="hljs-keyword">const</span></span><span> id = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"empanada-id"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>;
  </span><span><span class="hljs-keyword">const</span></span><span> data = {
    </span><span><span class="hljs-attr">name</span></span><span>: </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">"empanada-nombre"</span></span><span>).</span><span><span class="hljs-property">value</span></span><span>,
