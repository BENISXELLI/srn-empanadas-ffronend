    </span><span><span class="hljs-keyword">const</span></span><span> res = </span><span><span class="hljs-keyword">await</span></span><span> </span><span><span class="hljs-title function_">request</span></span><span>(app).</span><span><span class="hljs-title function_">delete</span></span><span>(</span><span><span class="hljs-string">`/api/empanada/<span class="hljs-subst">${createdId}</span></span></span><span>`);
